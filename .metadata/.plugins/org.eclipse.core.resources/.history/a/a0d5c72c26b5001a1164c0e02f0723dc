package com.tcs.model;

public class SoftwareEngineer {
	private String name;
	private int id;
	private boolean isHavingAge;
	public SoftwareEngineer(String name, int id, boolean isHavingAge) {
		super();
		this.name = name;
		this.id = id;
		this.isHavingAge = isHavingAge;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public boolean isHavingAge() {
		return isHavingAge;
	}
	public void setHavingAge(boolean isHavingAge) {
		this.isHavingAge = isHavingAge;
	}
	@Override
	public String toString() {
		return "SoftwareEngineer [name=" + name + ", id=" + id + ", isHavingAge=" + isHavingAge + "]";
	}
	

}
