/*
 * 
 * Program to remove null values and empty string in given array
 * */

package com.tcs.predicates;

import java.util.function.Predicate;

public class PredicateToRemoveNullValues {
	private static void removeNullEmptyvalues(Predicate<String> removevalues, String[] names) {

	}

	public static void main(String[] args) {
		String[] names = { "lokesh", "rajesh", " ", "null", "gouri" };
		Predicate<String> removevalues = name -> names != null && names.length != 0;
		removeNullEmptyvalues(removevalues, names);
	}

}
